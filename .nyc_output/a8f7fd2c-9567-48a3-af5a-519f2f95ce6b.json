r\n                    });\n                } else {\n                    const user = new User({\n                        _id: new mongoose.Types.ObjectId(), \n                        email: req.body.email,\n                        password: hash\n                    });\n                    user\n                    .save()\n                    .then(result => {\n                        console.log(result);\n                        res.status(201).json({\n                            message: 'User created'\n                        });  \n                    })\n                    .catch(err => {\n                        console.log(err);\n                        res.status(500).json({\n                            error: err\n                        });\n                    });\n                }\n            })\n        }\n     }) \n}\nexport default userSignUp"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"e8c6a77da1bfff7352b00d236e6441f26feaec3d","contentHash":"e4e0fa73a55e9ab2381f45f954df167a1979b3142b5ed8306b13c495b774d70b"},"/Users/andela/Desktop/Mybrand/server/models/user.js":{"path":"/Users/andela/Desktop/Mybrand/server/models/user.js","statementMap":{"0":{"start":{"line":3,"column":16},"end":{"line":3,"column":59}},"1":{"start":{"line":5,"column":39},"end":{"line":5,"column":95}},"2":{"start":{"line":7,"column":17},"end":{"line":19,"column":2}},"3":{"start":{"line":21,"column":0},"end":{"line":21,"column":64}}},"fnMap":{"0":{"name":"_interopRequireDefault","decl":{"start":{"line":5,"column":9},"end":{"line":5,"column":31}},"loc":{"start":{"line":5,"column":37},"end":{"line":5,"column":97}},"line":5}},"branchMap":{"0":{"loc":{"start":{"line":5,"column":46},"end":{"line":5,"column":94}},"type":"cond-expr","locations":[{"start":{"line":5,"column":70},"end":{"line":5,"column":73}},{"start":{"line":5,"column":76},"end":{"line":5,"column":94}}],"line":5},"1":{"loc":{"start":{"line":5,"column":46},"end":{"line":5,"column":67}},"type":"bi